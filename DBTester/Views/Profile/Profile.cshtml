@model IEnumerable<DBTester.Models.Profile>
@{
    ViewBag.Title = "Profile Viewer";
}

<head>
    <!-- DataTables -->
    <link href="~/assets/plugins/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/plugins/datatables/buttons.bootstrap4.min.css" rel="stylesheet" type="text/css" />
</head>

<div class="row">
    <div class="col-xl-12">
        <div class="card ">
            <div class="card-body">
                <h2>Profile Viewer</h2>
                <br />
                <div class="table-responsive">
                    <table id="datatable2" class="table border-0 dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>
                            <tr>
                                <th>Profile User</th>
                                <th>Items</th>
                                <th>Profit</th>
                                <th>Markdown</th>
                                <th>Shipping</th>
                                <th>Fee</th>
                                <th>Min</th>
                                <th>Max</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var profile in Model)
                            {
                                <tr>
                                    <td>
                                        @profile.ProfileUser
                                    </td>
                                    <td>
                                        @profile.items
                                    </td>
                                    <td>
                                        @profile.profit
                                    </td>
                                    <td>
                                        @profile.markdown
                                    </td>
                                    <td>
                                        @profile.shipping
                                    </td>
                                    <td>
                                        @profile.fee
                                    </td>
                                    <td>
                                        @profile.min
                                    </td>
                                    <td>
                                        @profile.max
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xl-12">
        <div class="card ">
            <div class="card-body">
                <h2>Title Viewer</h2>
                <div class="table-responsive">
                    <table id="datatable2" class="table border-0 dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>
                            <tr>
                                <th>User</th>
                                <th>Long Title Start</th>
                                <th>Mid Title Start</th>
                                <th>Short Title Start</th>
                                <th>Title End</th>
                                <th>Size Divider</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ProfileUser)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.LongstartTitle)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.MidtartTitle)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ShortstartTitle)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.endTtile)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.sizeDivider)
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div> 